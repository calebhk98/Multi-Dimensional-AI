[run]
source = src
branch = True
omit =
    */tests/*
    */__pycache__/*
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
precision = 2

fail_under = 70

[html]
directory = htmlcov
title = Multi-Dimensional AI Coverage Report

[xml]
output = coverage.xml
